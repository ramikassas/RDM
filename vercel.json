
{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "SUPABASE_URL": "Must be set in Vercel Dashboard",
    "SUPABASE_ANON_KEY": "Must be set in Vercel Dashboard"
  }
}
